@model IEnumerable<DifferentWeb.Models.Subject>

@{
    ViewBag.Title = "StudentSubjects";
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}

<h2>StudentSubjects</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Branch.BranchName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Professor.Qualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Semester.semester)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubjectName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Branch.BranchName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Professor.Qualification)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Semester.semester)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubjectName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
