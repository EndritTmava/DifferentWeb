@model IEnumerable<DifferentWeb.Models.Gradeing>

@{
    ViewBag.Title = "ReadGrades";
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}

<h2>My Grades</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Subject.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject.SubjectName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GradingDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Result)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Subject.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject.SubjectName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GradingDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result)
            </td>
        </tr>
    }

</table>

